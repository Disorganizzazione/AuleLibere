<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.harisont.aulelibere.Floor2">

    <ImageView
        android:id="@+id/mask2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitCenter"
        android:src="@drawable/duem"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/floor2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitCenter"
        android:src="@drawable/due"
        />
    <ImageView
        android:id="@+id/ag2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitCenter"
        android:src="@drawable/dueag"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/ar2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitCenter"
        android:src="@drawable/duear"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/bg2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitCenter"
        android:src="@drawable/duebg"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/br2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitCenter"
        android:src="@drawable/duebr"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/cg2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitCenter"
        android:src="@drawable/duecg"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/cr2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitCenter"
        android:src="@drawable/duecr"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/dr2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitCenter"
        android:src="@drawable/duedr"
        android:visibility="invisible"
        />
    <ImageView
        android:id="@+id/dg2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitCenter"
        android:src="@drawable/duedg"
        android:visibility="invisible"
        />
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:text="Piano 2" />


</RelativeLayout>
